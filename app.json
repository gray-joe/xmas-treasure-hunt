{
  "expo": {
    "name": "xmas-treasure-hunt",
    "slug": "xmas-treasure-hunt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_logo.png",
    "scheme": "xmastreasurehunt",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/app_logo.png"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1a4d2e",
        "foregroundImage": "./assets/images/app_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/app_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1a4d2e",
          "dark": {
            "backgroundColor": "#1a4d2e"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e28c82e2-d1a5-449a-8736-badd46585bf7"
      }
    }
  }
}
